  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "request": "^2.79.0",
    "node-ssdp": "^3.2.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "chalk": "^2.1.0",
    "clear": "0.0.1",
    "clui": "^0.3.6",
    "commander": "^2.11.0",
    "eslint": "^4.11.0",
    "eslint-config-google": "^0.9.1",
    "figlet": "^1.2.0",
    "fs": "0.0.1-security",
    "inquirer": "^3.2.3",
    "minimist": "^1.2.0",
    "mocha": "^3.5.0",
    "nock": "^9.0.14"
  }
